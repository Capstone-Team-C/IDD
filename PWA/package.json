{
  "name": "Vue-PWA-Happy-Path",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "serve -s dist",
    "postinstall": "npm run build",
    "start": "node server.js",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "jest-junit": "^10.0.0",
    "register-service-worker": "^1.6.2",
    "serve-static": "^1.14.1",
    "sinon": "^9.0.2",
    "vue": "^2.6.11",
    "vue-material": "^1.0.0-beta-11",
    "vue-router": "^3.1.6",
    "vue-upload-component": "^2.8.20",
    "vue-web-cam": "^1.9.0",
    "vuelidate": "^0.7.5",
    "vuetify": "^2.2.20"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.9.0",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-pwa": "^4.2.3",
    "@vue/cli-service": "~4.2.0",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.2.6",
    "deepmerge": "^4.2.2",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "express": "^4.17.1",
    "fibers": "^4.0.2",
    "jest": "^25.2.7",
    "node-sass": "^4.13.1",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "serve-static": "^1.14.1",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.vue$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
	"prettier": {
		"jsxBracketSameLine": false,
		"tabWidth": 2,
		"vueIndentScriptAndStyle": true,
		"useTabs": false
	},
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}

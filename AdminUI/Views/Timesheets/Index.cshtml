@model IEnumerable<AdminUI.Models.Timesheet>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ClientName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ClientPrime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProviderName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProviderID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CMOrg)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SCPAName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Service)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModCD)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Units)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Freq)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Hours)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ServiceGoal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProgressNotes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ClientSignature)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ClientSigned)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProviderSignature)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProviderSigned)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Submitted)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ClientName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClientPrime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProviderName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProviderID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CMOrg)

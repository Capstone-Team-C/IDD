                @Html.DisplayFor(modelItem => item.SCPAName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Service)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModCD)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Units)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Freq)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Hours)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceGoal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProgressNotes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClientSignature)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClientSigned)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProviderSignature)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProviderSigned)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Submitted)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TimesheetID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TimesheetID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TimesheetID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
